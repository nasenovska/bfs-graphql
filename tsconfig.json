{
    "compilerOptions": {
        "target": "es2018",
        "module": "commonjs",
        "lib": [
          "es2018",
          "esnext.asynciterable",
          "dom"
        ],
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "outDir": "./build"
    },
    "exclude": [
        "./node_modules",
        "./build"
    ]
}
